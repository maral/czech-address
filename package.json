{
  "name": "czech-address",
  "version": "1.0.1",
  "description": "NPM library for composing Czech addresses according to the law \"Vyhláška č. 359/2011 Sb.\"",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all clean -p build:*",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:js": "tsc --outDir dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maral/czech-address.git"
  },
  "keywords": [
    "address",
    "czech-republic",
    "czechia",
    "ruian",
    "Vyhláška č. 359/2011 Sb."
  ],
  "author": "Marek Lisý",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maral/czech-address/issues"
  },
  "homepage": "https://github.com/maral/czech-address#readme",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
